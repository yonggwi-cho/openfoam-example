{
    version 2.0;
    format ascii;
    class dictionary;
    location system;
    object projectDict;
}

    runDict
    {
        serverState
        {
            command RUN_CASE;
            solverState ERROR;
        }

        queue false;
        queueParameters
        {
            numberOfNodes 1;
            cpuPerNode 1;
            timeout 500;
            feature ;
            nodeNames ;
        }

        logFile simpleFoam.log;
        hostfilePath ;
        multiMachine false;
        sshParameters
        {
            openFoamDir ;
            user ;
            sshauth SSH_PWD;
            sshkey ;
            sshpwd ;
            remoteBaseDir ;
            remoteBaseDirParent ;
            paraviewDir ;
            applicationDir ;
            validForRemoteChooser true;
            host ;
            port 22;
        }

        remote false;
    }

    meshInfo
    {
        valid false;
        faceZones -1;
        cellZones -1;
        cells -1;
        points -1;
        faces -1;
        internalFaces -1;
